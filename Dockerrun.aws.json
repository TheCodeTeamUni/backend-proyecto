{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "users_app",
            "image": "691179146525.dkr.ecr.ap-southeast-1.amazonaws.com/users_app:latest",
            "hostname": "users",
            "essential": true,
            "memory": 1024,
            "portMappings": [
                {
                    "hostPort": 3001,
                    "containerPort": 3001
                }
            ]
        },
        {
            "name": "nginx",
            "image": "nginx:latest",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["users_app"],
            "memory": 512
        }
    ]
}
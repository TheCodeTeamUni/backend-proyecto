{
    "executionRoleArn": "arn:aws:iam::729930084599:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],

        "image": "<IMAGE1_NAME>",
        "essential": true,
        "name": "orch_app"
      },
      {
        "portMappings": [
          {
            "hostPort": 3001,
            "protocol": "tcp",
            "containerPort": 3001
          }
        ],

        "image": "<IMAGE2_NAME>",
        "essential": false,
        "name": "users_app"
      }
    ],

    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "Task-jobs-app"
  }